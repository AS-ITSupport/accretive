#homemainBox {
  padding: 0 12px 0 8px;
  margin: 0;
  background: transparent url("../../images/home-bg.png") 8px top no-repeat;	
}

#homeContent {
  background: transparent url("../../images/home-footer-bg.gif") left bottom no-repeat;	
  padding: 0;
  margin: 0;
	border-bottom: 2px solid #fff;
}

#sloganBox {
  padding: 0;
  margin: 0;
  float: right;
  width: 345px;
}

.slogan  {
  font-size: 30px;
  color: #153692;
  padding: 150px 0 0 0;
}

#homeColContent {
  padding: 110px 0 10px 0;
  margin: 0;
  float: left;
  width: 100%;
}

#homeColContent .block {
    float: left;
    width: 175px;
    padding: 0 0 0 0;
}

#homeColContent .block .content {
    padding: 0 10px 0 0;
}

#homeColContent .block h2 {
	font-size: 10px;
	color: #153692;
	font-weight: bold;
	text-transform: uppercase;
	letter-spacing: 1px;
	border:none;
	padding-top:3px;
	margin-bottom:0px;
	padding-bottom:2px;
}


#homeColContent .block .content ul{
    padding: 10px 0 0 0;
    margin: 0;
}

#homeColContent .block .content li{
    padding: 0 0 8px 0;
    margin: 0;
}

.learnMore {
  padding: 20px 0 0 0;
  margin: 0;
}

#homeColRight {
  float: right;
  width: 350px;
}

#homeNews {
  margin: 0;
  padding: 45px 0 0 0;
  float: left;
  width: 100%;
}

#homeNews .block .content ul {
  list-style: none;
  color: #fff;
  padding: 0 0 20px 0;
  margin: 0;
  line-height: 15px;   
}

#homeNews .block .content li {
  list-style: none;
  padding: 0 10px 5px 0;
  margin: 0;
  font-size: 12px;
}

#homeNews .block .content li a {
  text-decoration: underline;
}

#homeNews .block h2 {    
  background: transparent url("../../images/home-title-news-bg.png") left top no-repeat;
  padding: 5px 100px 3px 8px;  
  margin: 0 0 12px 0;
  color: #153692;
  font-weight: bold;
  font-size: 10px;
  text-transform: uppercase;
  letter-spacing: 1px;
  border-bottom: 1px solid #fff;
	line-height:12px;
}

#homeNews .block .content a {
    color: #fff;
    text-decoration: none;
}

#homeNews .block .content a:hover {
    text-decoration: underline;
}

#homeNews .block .content li b {

}

b {
  font-weight: bold;
  font-size: 12px;
  padding-bottom: 100px;
}

/** Drupal Overrides **/
.view-home-news .date-display-single {
  font-weight: bold;    
}

.view-home-news .views-field-view-node a {
	color:#fff;
}

#homeColContent .block {
  margin: 0;
  padding: 0;
}

#homeFlash {
  float: left;
  padding: 9px 0 0 4px;
}
/** End of Drupal Overrides **/